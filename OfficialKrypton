--[/]Humanoid	
PlayerTab:AddLabel("Humanoid")
local WalkSpeed = PlayerTab:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(Value)
	  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})
local JumpPower = PlayerTab:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump power",
	Callback = function(Value)
	  game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})
PlayerTab:AddButton({
	Name = "Reset WalkSpeed",
	Callback = function()
	 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	 WalkSpeed:Set(16)
  	end
})
PlayerTab:AddButton({
	Name = "Reset Jump power",
	Callback = function()
	 game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	 JumpPower:Set(50)
  	end   
})   
PlayerTab:AddButton({
	Name = "Restore to default configuration",
	Callback = function()
	 --[/]Jump power
	 game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	 JumpPower:Set(50)
	 --[/]Walkspeed
	 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	 WalkSpeed:Set(16)
  	end
})
--[/] ORION
OrionLib:Init()
