--[/] Orion Library | Mobile / PC
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
--[/] Opening MakeNotification
OrionLib:MakeNotification({
	Name = "Krypton",
	Content = "Ran Krypton successfully",
	Image = "rbxassetid://4483345998",
	Time = 5.5
  })
--[/] Functions
function closenotify()
  OrionLib:MakeNotification({
	Name = "Krypton",
	Content = "Thank you for using Krypton!",
	Image = "rbxassetid://4483345998",
	Time = 5.5
  })
end
--[/] Window
local Window = OrionLib:MakeWindow({
  Name = "Krypton | HUB", 
  HidePremium = false, 
  SaveConfig = true,
  IntroEnabled = true,
  IntroText = "Krypton",
  ConfigFolder = "KryptonFOLDER",
})
--[/] Tabs
local RatemyAvatarTab = Window:MakeTab({
	Name = "Rate my Avatar",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
